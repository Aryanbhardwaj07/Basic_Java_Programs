public class LargestArrayProgm {
    public static void main(String[] args) {
        int i;
        int arr[] = {10,20,30,60,50};
        int largest = arr[0];   
        
        for(i=0;i<arr.length;i++){
            if(arr[i]> largest){
                largest = arr[i];
            }
        }
        System.out.println(largest);
    }
    
    
}
